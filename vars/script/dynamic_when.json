{
  "run": [{
    "when": "{{script.exists('https://github.com/cocktailpeanutlabs/bark.git/env')}}",
    "method": "log",
    "params": {
      "raw": "already exists. skip."
    }
  }, {
    "when": "{{script.exists('https://github.com/cocktailpeanutlabs/bark.git/env')}}",
    "method": "goto",
    "params": {
      "index": null
    }
  }, {
    "when": "{{!script.exists('https://github.com/cocktailpeanutlabs/bark.git/env')}}",
    "method": "log",
    "params": {
      "raw": "does not exist. installing..."
    }
  }, {
    "when": "{{!script.exists('https://github.com/cocktailpeanutlabs/bark.git/env')}}",
    "method": "script.start",
    "params": {
      "uri": "https://github.com/cocktailpeanutlabs/bark.git/install.js"
    }
  }, {
    "when": "{{!script.exists('https://github.com/cocktailpeanutlabs/bark.git/env')}}",
    "method": "log",
    "params": {
      "raw": "after install: {{script.exists('https://github.com/cocktailpeanutlabs/bark.git/env')}}"
    }
  }]
}
